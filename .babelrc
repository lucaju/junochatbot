{
	"presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage", // or "entry"
            "corejs": 3
          }
        ],
        "@babel/preset-react"
      ],
	"plugins": [[
        "@babel/plugin-transform-runtime",
        {
            "corejs": { "version": 3, "proposals": true },
            "helpers": true,
            "regenerator": true,
            "useESModules": true,
            "version": "^7.14.0"
        }
    ]]
}
